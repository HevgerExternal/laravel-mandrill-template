{
	"name": "addgod/laravel-mandrill-template",
	"description": "Using Mandrill templates in Laravel.",
	"keywords": [
		"laravel",
		"nova"
	],
	"license": "MIT",
	"require": {
		"php": "^7.3|^8.0",
		"mandrill/mandrill": "^1.0",
		"ext-fileinfo": "*"
	},
	"autoload": {
		"psr-4": {
			"Addgod\\MandrillTemplate\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Addgod\\MandrillTemplate\\MandrillTemplateServiceProvider"
			]
		}
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
